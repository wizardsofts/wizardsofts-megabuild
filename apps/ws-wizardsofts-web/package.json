{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "prepare": "husky",
    "test": "jest --config=jest.config.js --passWithNoTests",
    "test:watch": "jest --watch",
    "test:contract": "jest --testPathPatterns=contract --passWithNoTests",
    "lhci": "lhci autorun",
    "axe-check": "node scripts/axe-ci.js",
    "docker:build": "./build-and-push.sh",
    "docker:build:tag": "./build-and-push.sh"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "16.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axe-core": "^4.10.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "puppeteer": "^24.23.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.4",
    "typescript": "^5"
  }
}
