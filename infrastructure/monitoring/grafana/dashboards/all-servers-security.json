{
  "uid": "security-overview-all",
  "title": "All Servers Security Overview",
  "description": "Combined security monitoring for all WizardSofts servers",
  "tags": ["security", "overview", "monitoring"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "1m",
  "schemaVersion": 38,
  "version": 1,
  "time": {"from": "now-24h", "to": "now"},
  "panels": [
    {
      "id": 1, "type": "row", "title": "Server 80 (GIBD Services)",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1}, "collapsed": false
    },
    {
      "id": 2, "type": "stat", "title": "S80 Status",
      "gridPos": {"x": 0, "y": 1, "w": 3, "h": 3},
      "targets": [{"expr": "security_scan_status{job=\"security-80\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"1": {"text": "SECURE", "color": "green"}, "0": {"text": "ALERT", "color": "red"}}}], "thresholds": {"mode": "absolute", "steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 3, "type": "stat", "title": "S80 Critical CVEs",
      "gridPos": {"x": 3, "y": 1, "w": 3, "h": 3},
      "targets": [{"expr": "security_docker_vulnerabilities{severity=\"critical\",job=\"security-80\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 1}, {"color": "red", "value": 5}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
