{
  "realm": "wizardsofts",
  "enabled": true,
  "displayName": "WizardSofts Platform",
  "displayNameHtml": "<div class=\"kc-logo-text\"><span>WizardSofts</span></div>",
  "registrationAllowed": false,
  "registrationEmailAsUsername": true,
  "rememberMe": true,
  "verifyEmail": true,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "sslRequired": "external",
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 600,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 5,
  "accessTokenLifespan": 300,
  "accessTokenLifespanForImplicitFlow": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "ssoSessionIdleTimeoutRememberMe": 0,
  "ssoSessionMaxLifespanRememberMe": 0,
  "offlineSessionIdleTimeout": 2592000,
  "offlineSessionMaxLifespanEnabled": true,
  "offlineSessionMaxLifespan": 2592000,
  "clientSessionIdleTimeout": 0,
  "clientSessionMaxLifespan": 0,
  "clientOfflineSessionIdleTimeout": 0,
  "clientOfflineSessionMaxLifespan": 0,
  "accessCodeLifespan": 60,
  "accessCodeLifespanUserAction": 300,
  "accessCodeLifespanLogin": 1800,
  "actionTokenGeneratedByAdminLifespan": 43200,
  "actionTokenGeneratedByUserLifespan": 300,
  "oauth2DeviceCodeLifespan": 600,
  "oauth2DevicePollingInterval": 5,
  "defaultSignatureAlgorithm": "RS256",
  "revokeRefreshToken": true,
  "refreshTokenMaxReuse": 0,
  "passwordPolicy": "length(12) and digits(1) and lowerCase(1) and upperCase(1) and specialChars(1) and notUsername and passwordHistory(5)",
  "smtpServer": {},
  "eventsEnabled": true,
  "eventsListeners": [
    "jboss-logging",
    "metrics-listener"
  ],
  "enabledEventTypes": [
    "LOGIN",
    "LOGOUT",
    "LOGIN_ERROR",
    "REGISTER",
    "UPDATE_PROFILE",
    "CLIENT_LOGIN",
    "TOKEN_EXCHANGE",
    "CODE_TO_TOKEN",
    "CODE_TO_TOKEN_ERROR",
    "REFRESH_TOKEN",
    "REFRESH_TOKEN_ERROR",
    "INTROSPECT_TOKEN",
    "INTROSPECT_TOKEN_ERROR",
    "FEDERATED_IDENTITY_LINK",
    "REMOVE_FEDERATED_IDENTITY",
    "LOGOUT_ERROR",
    "REGISTER_ERROR"
  ],
  "adminEventsEnabled": true,
  "adminEventsDetailsEnabled": true,
  "roles": {
    "realm": [
      {
        "name": "super-admin",
        "description": "Platform administrators - Full system access, manage tenants",
        "composite": false
      },
      {
        "name": "tenant-admin",
        "description": "Tenant administrators - Manage users within tenant, configure tenant settings",
        "composite": false
      },
      {
        "name": "user",
        "description": "Standard user - Access to assigned applications",
        "composite": false
      },
      {
        "name": "viewer",
        "description": "Read-only user - View-only access",
        "composite": false
      }
    ]
  },
  "groups": [
    {
      "name": "tenants",
      "path": "/tenants",
      "subGroups": []
    },
    {
      "name": "internal",
      "path": "/internal",
      "attributes": {
        "tenant_id": ["wizardsofts"],
        "tenant_plan": ["internal"]
      },
      "realmRoles": ["super-admin"]
    }
  ],
  "defaultDefaultClientScopes": [
    "role_list",
    "profile",
    "email",
    "roles",
    "web-origins",
    "acr"
  ],
  "defaultOptionalClientScopes": [
    "offline_access",
    "address",
    "phone",
    "microprofile-jwt"
  ],
  "clientScopes": [
    {
      "name": "tenant_info",
      "description": "Tenant information claims",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      },
      "protocolMappers": [
        {
          "name": "tenant_id",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "tenant_id",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "tenant_id",
            "jsonType.label": "String"
          }
        },
        {
          "name": "tenant_plan",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "userinfo.token.claim": "true",
            "user.attribute": "tenant_plan",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "tenant_plan",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "gibd-quant-web",
      "name": "GIBD Quant Web Application",
      "description": "Frontend application for GIBD Quant platform",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "redirectUris": [
        "https://quant.wizardsofts.com/api/auth/callback/keycloak",
        "http://localhost:3000/api/auth/callback/keycloak"
      ],
      "webOrigins": [
        "https://quant.wizardsofts.com",
        "http://localhost:3000"
      ],
      "publicClient": true,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "tenant_info"
      ],
      "optionalClientScopes": [
        "offline_access"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "oauth2.device.authorization.grant.enabled": "false",
        "oidc.ciba.grant.enabled": "false",
        "post.logout.redirect.uris": "https://quant.wizardsofts.com/*##http://localhost:3000/*",
        "access.token.lifespan": "300"
      }
    },
    {
      "clientId": "ws-gateway",
      "name": "WS Gateway Resource Server",
      "description": "API Gateway for microservices",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${WS_GATEWAY_CLIENT_SECRET}",
      "redirectUris": [],
      "webOrigins": [],
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "tenant_info"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "ws-trades-service",
      "name": "WS Trades Microservice",
      "description": "Service-to-service client for trades service",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${WS_TRADES_CLIENT_SECRET}",
      "redirectUris": [],
      "webOrigins": [],
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "openid",
        "tenant_info"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "ws-company-service",
      "name": "WS Company Microservice",
      "description": "Service-to-service client for company service",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${WS_COMPANY_CLIENT_SECRET}",
      "redirectUris": [],
      "webOrigins": [],
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "openid",
        "tenant_info"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "ws-news-service",
      "name": "WS News Microservice",
      "description": "Service-to-service client for news service",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${WS_NEWS_CLIENT_SECRET}",
      "redirectUris": [],
      "webOrigins": [],
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": false,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": false,
      "defaultClientScopes": [
        "openid",
        "tenant_info"
      ],
      "optionalClientScopes": []
    },
    {
      "clientId": "grafana",
      "name": "Grafana Dashboard",
      "description": "OAuth client for Grafana SSO",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "${GRAFANA_OAUTH_CLIENT_SECRET}",
      "redirectUris": [
        "https://grafana.wizardsofts.com/login/generic_oauth",
        "http://10.0.0.84:3002/login/generic_oauth"
      ],
      "webOrigins": [
        "https://grafana.wizardsofts.com",
        "http://10.0.0.84:3002"
      ],
      "publicClient": false,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "fullScopeAllowed": true,
      "defaultClientScopes": [
        "openid",
        "profile",
        "email",
        "roles"
      ],
      "optionalClientScopes": [],
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    }
  ],
  "requiredActions": [
    {
      "alias": "CONFIGURE_TOTP",
      "name": "Configure OTP",
      "providerId": "CONFIGURE_TOTP",
      "enabled": true,
      "defaultAction": false,
      "priority": 10,
      "config": {}
    },
    {
      "alias": "UPDATE_PASSWORD",
      "name": "Update Password",
      "providerId": "UPDATE_PASSWORD",
      "enabled": true,
      "defaultAction": false,
      "priority": 30,
      "config": {}
    },
    {
      "alias": "VERIFY_EMAIL",
      "name": "Verify Email",
      "providerId": "VERIFY_EMAIL",
      "enabled": true,
      "defaultAction": true,
      "priority": 50,
      "config": {}
    }
  ],
  "browserSecurityHeaders": {
    "contentSecurityPolicyReportOnly": "",
    "xContentTypeOptions": "nosniff",
    "referrerPolicy": "no-referrer",
    "xRobotsTag": "none",
    "xFrameOptions": "SAMEORIGIN",
    "contentSecurityPolicy": "frame-src 'self'; frame-ancestors 'self'; object-src 'none';",
    "xXSSProtection": "1; mode=block",
    "strictTransportSecurity": "max-age=31536000; includeSubDomains"
  },
  "attributes": {
    "frontendUrl": "https://id.wizardsofts.com",
    "acr.loa.map": "{\"gold\":\"3\",\"silver\":\"2\",\"bronze\":\"1\"}",
    "oauth2.device.authorization.grant.enabled": "false",
    "oidc.ciba.grant.enabled": "false",
    "pkce.code.challenge.required": "true"
  }
}
