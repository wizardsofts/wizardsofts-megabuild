{
  "uid": "dba-dashboard",
  "title": "ðŸ—„ï¸ DBA - Database Performance & Health",
  "description": "Database administrator dashboard: PostgreSQL, Redis metrics, replication, queries",
  "tags": ["dba", "database", "postgresql", "redis", "performance"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "database",
        "type": "query",
        "query": "label_values(pg_stat_database_tup_fetched, datname)",
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "ðŸ˜ PostgreSQL - Connection & Status",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "PostgreSQL Status",
      "description": "Database server availability",
      "gridPos": {"x": 0, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "pg_up",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"1": {"text": "ONLINE", "color": "green"}}},
            {"type": "value", "options": {"0": {"text": "OFFLINE", "color": "red"}}}
          ],
          "noValue": "N/A"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Active Connections",
      "description": "Currently active database connections",
      "gridPos": {"x": 4, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname!~\"template.*|postgres\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{datname}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "orange", "value": 80},
              {"color": "red", "value": 95}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 4,
      "type": "gauge",
      "title": "Connection Pool Usage",
      "description": "Connections used vs max_connections (typically 100)",
      "gridPos": {"x": 8, "y": 1, "w": 8, "h": 4},
      "targets": [
        {
          "expr": "(sum(pg_stat_database_numbackends{datname!~\"template.*|postgres\"}) / pg_settings_max_connections) * 100",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "orange", "value": 80},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Connection Count Over Time",
      "gridPos": {"x": 16, "y": 1, "w": 8, "h": 4},
      "targets": [
        {
          "expr": "pg_stat_database_numbackends{datname!~\"template.*|postgres\"}",
          "refId": "A",
          "legendFormat": "{{datname}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 6,
      "type": "row",
      "title": "ðŸ“Š Query Performance",
      "gridPos": {"x": 0, "y": 5, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Queries Per Second",
      "description": "Total database queries/sec across all databases",
      "gridPos": {"x": 0, "y": 6, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_commit{datname=~\"$database\"}[5m]) + rate(pg_stat_database_xact_rollback{datname=~\"$database\"}[5m]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Transaction Commit Rate",
      "gridPos": {"x": 4, "y": 6, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_commit{datname=~\"$database\"}[5m]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Transaction Rollback Rate",
      "gridPos": {"x": 8, "y": 6, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(rate(pg_stat_database_xact_rollback{datname=~\"$database\"}[5m]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 10}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Query Execution Rate by Database",
      "gridPos": {"x": 12, "y": 6, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(pg_stat_database_xact_commit{datname=~\"$database\"}[5m])",
          "refId": "A",
          "legendFormat": "{{datname}} - commits"
        },
        {
          "expr": "rate(pg_stat_database_xact_rollback{datname=~\"$database\"}[5m])",
          "refId": "B",
          "legendFormat": "{{datname}} - rollbacks"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Deadlocks (24h)",
      "description": "Total deadlocks detected in the last 24 hours",
      "gridPos": {"x": 0, "y": 9, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(pg_stat_database_deadlocks{datname=~\"$database\"}[24h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 10}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 12,
      "type": "stat",
      "title": "Conflicts (24h)",
      "description": "Query conflicts from recovery or standby",
      "gridPos": {"x": 4, "y": 9, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(pg_stat_database_conflicts{datname=~\"$database\"}[24h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 20}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 13,
      "type": "stat",
      "title": "Temp Files Created (1h)",
      "description": "Queries using temp files (may indicate memory pressure)",
      "gridPos": {"x": 8, "y": 9, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(pg_stat_database_temp_files{datname=~\"$database\"}[1h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 100}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 14,
      "type": "row",
      "title": "ðŸ’¾ Cache & I/O Performance",
      "gridPos": {"x": 0, "y": 12, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 15,
      "type": "gauge",
      "title": "Cache Hit Ratio",
      "description": "Buffer cache hit percentage (target: >99%)",
      "gridPos": {"x": 0, "y": 13, "w": 8, "h": 5},
      "targets": [
        {
          "expr": "100 * (sum(pg_stat_database_blks_hit{datname=~\"$database\"}) / (sum(pg_stat_database_blks_hit{datname=~\"$database\"}) + sum(pg_stat_database_blks_read{datname=~\"$database\"})))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 90,
          "max": 100,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 95},
              {"color": "green", "value": 99}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 16,
      "type": "timeseries",
      "title": "Disk Blocks Read vs Cache Hits",
      "gridPos": {"x": 8, "y": 13, "w": 16, "h": 5},
      "targets": [
        {
          "expr": "rate(pg_stat_database_blks_read{datname=~\"$database\"}[5m])",
          "refId": "A",
          "legendFormat": "{{datname}} - disk reads"
        },
        {
          "expr": "rate(pg_stat_database_blks_hit{datname=~\"$database\"}[5m])",
          "refId": "B",
          "legendFormat": "{{datname}} - cache hits"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 17,
      "type": "row",
      "title": "ðŸ“¦ Database Size & Growth",
      "gridPos": {"x": 0, "y": 18, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 18,
      "type": "bargauge",
      "title": "Database Size",
      "gridPos": {"x": 0, "y": 19, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "pg_database_size_bytes{datname=~\"$database\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{datname}}"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10737418240},
              {"color": "red", "value": 53687091200}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 19,
      "type": "timeseries",
      "title": "Database Growth Rate",
      "gridPos": {"x": 12, "y": 19, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "deriv(pg_database_size_bytes{datname=~\"$database\"}[1h])",
          "refId": "A",
          "legendFormat": "{{datname}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 20,
      "type": "row",
      "title": "ðŸ”´ Redis Cache",
      "gridPos": {"x": 0, "y": 25, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 21,
      "type": "stat",
      "title": "Redis Status",
      "gridPos": {"x": 0, "y": 26, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "redis_up",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"1": {"text": "UP", "color": "green"}}},
            {"type": "value", "options": {"0": {"text": "DOWN", "color": "red"}}}
          ],
          "noValue": "N/A"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 22,
      "type": "stat",
      "title": "Redis Memory Used",
      "gridPos": {"x": 3, "y": 26, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 429496729},
              {"color": "red", "value": 536870912}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 23,
      "type": "stat",
      "title": "Redis Keys",
      "gridPos": {"x": 6, "y": 26, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "sum(redis_db_keys)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 24,
      "type": "stat",
      "title": "Redis Hit Rate",
      "description": "Cache hit percentage",
      "gridPos": {"x": 9, "y": 26, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "100 * (redis_keyspace_hits_total / (redis_keyspace_hits_total + redis_keyspace_misses_total))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "green", "value": 90}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 25,
      "type": "timeseries",
      "title": "Redis Commands/sec",
      "gridPos": {"x": 12, "y": 26, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[5m])",
          "refId": "A",
          "legendFormat": "Commands/sec"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 26,
      "type": "stat",
      "title": "Redis Evicted Keys (1h)",
      "description": "Keys removed due to maxmemory",
      "gridPos": {"x": 0, "y": 29, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "increase(redis_evicted_keys_total[1h])",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 1000}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 27,
      "type": "stat",
      "title": "Redis Connected Clients",
      "gridPos": {"x": 4, "y": 29, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "redis_connected_clients",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 28,
      "type": "stat",
      "title": "Redis Blocked Clients",
      "description": "Clients blocked on BLPOP, BRPOP, etc.",
      "gridPos": {"x": 8, "y": 29, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "redis_blocked_clients",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 20}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    }
  ]
}
