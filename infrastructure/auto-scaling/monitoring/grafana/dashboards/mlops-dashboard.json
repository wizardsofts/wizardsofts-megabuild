    {
      "id": 27,
      "type": "stat",
      "title": "Pipeline Success Rate (24h)",
      "gridPos": {"x": 4, "y": 26, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "100 * (sum(increase(pipeline_jobs_succeeded[24h])) / (sum(increase(pipeline_jobs_succeeded[24h])) + sum(increase(pipeline_jobs_failed[24h]))))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
