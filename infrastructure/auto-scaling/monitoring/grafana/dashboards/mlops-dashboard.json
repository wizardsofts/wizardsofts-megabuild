{
  "uid": "mlops-dashboard",
  "title": "ðŸ¤– MLOps - Model Performance & ML Infrastructure",
  "description": "ML Operations: model inference, Ray cluster, Celery tasks, GPU utilization",
  "tags": ["mlops", "ml", "ai", "ray", "celery", "gpu"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "ðŸŽ¯ Model Inference Performance",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Inference Requests/min",
      "description": "Total model inference requests per minute",
      "gridPos": {"x": 0, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "sum(rate(model_inference_requests_total[1m])) * 60",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 3,
      "type": "stat",
      "title": "P50 Latency",
      "description": "Median inference time",
      "gridPos": {"x": 4, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 500},
              {"color": "red", "value": 1000}
            ]
          },
          "noValue": "N/A"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 4,
      "type": "stat",
      "title": "P95 Latency",
      "description": "95th percentile inference time",
      "gridPos": {"x": 8, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 2000}
            ]
          },
          "noValue": "N/A"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 5,
      "type": "stat",
      "title": "P99 Latency",
      "description": "99th percentile inference time",
      "gridPos": {"x": 12, "y": 1, "w": 4, "h": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2000},
              {"color": "red", "value": 5000}
            ]
          },
          "noValue": "N/A"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Inference Latency Percentiles",
      "gridPos": {"x": 16, "y": 1, "w": 8, "h": 4},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "A",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "B",
          "legendFormat": "P95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(model_inference_duration_seconds_bucket[5m])) by (le)) * 1000",
          "refId": "C",
          "legendFormat": "P99"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 7,
      "type": "row",
      "title": "ðŸŒŸ Ray Cluster Status",
      "gridPos": {"x": 0, "y": 5, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Ray Cluster Status",
      "gridPos": {"x": 0, "y": 6, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "ray_cluster_active_nodes",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "range", "options": {"from": 1, "to": 999, "result": {"text": "ACTIVE", "color": "green"}}},
            {"type": "special", "options": {"match": "null", "result": {"text": "OFFLINE", "color": "red"}}}
          ],
          "noValue": "OFFLINE"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Active Nodes",
      "gridPos": {"x": 3, "y": 6, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "ray_cluster_active_nodes",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 10,
      "type": "stat",
      "title": "Total CPUs",
      "gridPos": {"x": 6, "y": 6, "w": 3, "h": 3},
      "targets": [
        {
          "expr": "sum(ray_node_cpu_count)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 11,
      "type": "gauge",
      "title": "CPU Utilization",
      "gridPos": {"x": 9, "y": 6, "w": 5, "h": 3},
      "targets": [
        {
          "expr": "100 * (sum(ray_node_cpu_count) - sum(ray_node_cpu_available)) / sum(ray_node_cpu_count)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 12,
      "type": "gauge",
      "title": "Memory Utilization",
      "gridPos": {"x": 14, "y": 6, "w": 5, "h": 3},
      "targets": [
        {
          "expr": "100 * (sum(ray_node_mem_total) - sum(ray_node_mem_available)) / sum(ray_node_mem_total)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 80},
              {"color": "red", "value": 95}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 13,
      "type": "stat",
      "title": "Active Tasks",
      "gridPos": {"x": 19, "y": 6, "w": 5, "h": 3},
      "targets": [
        {
          "expr": "sum(ray_tasks_running)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 14,
      "type": "row",
      "title": "ðŸ”„ Celery Task Queue",
      "gridPos": {"x": 0, "y": 9, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 15,
      "type": "stat",
      "title": "Active Workers",
      "gridPos": {"x": 0, "y": 10, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "celery_workers_active",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "green", "value": 2}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 16,
      "type": "stat",
      "title": "Queue Depth",
      "description": "Tasks waiting in queue",
      "gridPos": {"x": 4, "y": 10, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(celery_queue_length)",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "orange", "value": 100},
              {"color": "red", "value": 500}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 17,
      "type": "stat",
      "title": "Tasks Succeeded (1h)",
      "gridPos": {"x": 8, "y": 10, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(celery_task_succeeded_total[1h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 18,
      "type": "stat",
      "title": "Tasks Failed (1h)",
      "gridPos": {"x": 12, "y": 10, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(celery_task_failed_total[1h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 20}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 19,
      "type": "timeseries",
      "title": "Task Execution Rate",
      "gridPos": {"x": 16, "y": 10, "w": 8, "h": 6},
      "targets": [
        {
          "expr": "sum(rate(celery_task_succeeded_total[5m]))",
          "refId": "A",
          "legendFormat": "Succeeded"
        },
        {
          "expr": "sum(rate(celery_task_failed_total[5m]))",
          "refId": "B",
          "legendFormat": "Failed"
        },
        {
          "expr": "sum(rate(celery_task_retried_total[5m]))",
          "refId": "C",
          "legendFormat": "Retried"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "list", "placement": "bottom"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 20,
      "type": "timeseries",
      "title": "Celery Queue Depth Over Time",
      "gridPos": {"x": 0, "y": 13, "w": 16, "h": 6},
      "targets": [
        {
          "expr": "celery_queue_length",
          "refId": "A",
          "legendFormat": "{{queue}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 21,
      "type": "row",
      "title": "ðŸŽ® GPU Utilization (if available)",
      "gridPos": {"x": 0, "y": 19, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 22,
      "type": "gauge",
      "title": "GPU Utilization",
      "gridPos": {"x": 0, "y": 20, "w": 8, "h": 5},
      "targets": [
        {
          "expr": "nvidia_smi_utilization_gpu_ratio * 100",
          "refId": "A",
          "instant": true,
          "legendFormat": "GPU {{gpu}}"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 95}
            ]
          },
          "noValue": "No GPU"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 23,
      "type": "gauge",
      "title": "GPU Memory Usage",
      "gridPos": {"x": 8, "y": 20, "w": 8, "h": 5},
      "targets": [
        {
          "expr": "(nvidia_smi_memory_used_bytes / nvidia_smi_memory_total_bytes) * 100",
          "refId": "A",
          "instant": true,
          "legendFormat": "GPU {{gpu}}"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 80},
              {"color": "red", "value": 95}
            ]
          },
          "noValue": "No GPU"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 24,
      "type": "stat",
      "title": "GPU Temperature",
      "gridPos": {"x": 16, "y": 20, "w": 8, "h": 5},
      "targets": [
        {
          "expr": "nvidia_smi_temperature_gpu",
          "refId": "A",
          "instant": true,
          "legendFormat": "GPU {{gpu}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "thresholds": {
            "mode": "absolute",
            "steps": [
