    {
      "id": 25,
      "type": "row",
      "title": "ðŸ“Š Data Pipeline Health",
      "gridPos": {"x": 0, "y": 25, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 26,
      "type": "stat",
      "title": "Pipeline Jobs Running",
      "gridPos": {"x": 0, "y": 26, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(pipeline_jobs_running)",
