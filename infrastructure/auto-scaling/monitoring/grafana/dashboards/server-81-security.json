{
  "uid": "security-overview-81",
  "title": "Server 81 Security Dashboard",
  "description": "Security monitoring for Server 81 (Database Server)",
  "tags": ["security", "server-81", "monitoring", "database"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "1m",
  "schemaVersion": 38,
  "version": 1,
  "time": {"from": "now-24h", "to": "now"},
  "panels": [
    {
      "id": 1, "type": "stat", "title": "Security Status",
      "gridPos": {"x": 0, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "security_scan_status{job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"1": {"text": "SECURE", "color": "green"}, "0": {"text": "ALERT", "color": "red"}}}], "thresholds": {"mode": "absolute", "steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 2, "type": "stat", "title": "Server Type",
      "gridPos": {"x": 4, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "1", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"1": {"text": "DATABASE", "color": "blue"}}}]}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 3, "type": "stat", "title": "Failed Logins",
      "gridPos": {"x": 8, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "security_failed_logins{type=\"total\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "red", "value": 50}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 4, "type": "stat", "title": "Suspicious Processes",
      "gridPos": {"x": 12, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "security_suspicious_processes{type=\"total\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "red", "value": 1}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 5, "type": "stat", "title": "Open Ports",
      "gridPos": {"x": 16, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "security_open_ports{type=\"total\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "red", "value": 30}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 6, "type": "stat", "title": "Last Scan",
      "gridPos": {"x": 20, "y": 0, "w": 4, "h": 4},
      "targets": [{"expr": "time() - security_scan_timestamp{job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "value"},
      "fieldConfig": {"defaults": {"unit": "s", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 1800}, {"color": "red", "value": 3600}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 7, "type": "gauge", "title": "Failed Login Attempts",
      "gridPos": {"x": 0, "y": 4, "w": 8, "h": 6},
      "targets": [{"expr": "security_failed_logins{type=\"total\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"showThresholdLabels": false, "showThresholdMarkers": true},
      "fieldConfig": {"defaults": {"unit": "short", "min": 0, "max": 100, "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "orange", "value": 50}, {"color": "red", "value": 100}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 8, "type": "gauge", "title": "Open Ports",
      "gridPos": {"x": 8, "y": 4, "w": 8, "h": 6},
      "targets": [{"expr": "security_open_ports{type=\"total\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"showThresholdLabels": false, "showThresholdMarkers": true},
      "fieldConfig": {"defaults": {"unit": "short", "min": 0, "max": 50, "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "orange", "value": 20}, {"color": "red", "value": 40}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 9, "type": "gauge", "title": "File Permission Issues",
      "gridPos": {"x": 16, "y": 4, "w": 8, "h": 6},
      "targets": [{"expr": "security_world_writable_files{job=\"security-81\"} + security_suid_files{job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"showThresholdLabels": false, "showThresholdMarkers": true},
      "fieldConfig": {"defaults": {"unit": "short", "min": 0, "max": 50, "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "red", "value": 30}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 10, "type": "bargauge", "title": "Failed Logins by Type",
      "gridPos": {"x": 0, "y": 10, "w": 12, "h": 6},
      "targets": [{"expr": "security_failed_logins{job=\"security-81\",type!=\"total\"}", "refId": "A", "legendFormat": "{{type}}", "instant": true}],
      "options": {"orientation": "horizontal", "displayMode": "gradient", "showUnfilled": true},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 5}, {"color": "red", "value": 20}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 11, "type": "bargauge", "title": "File Permissions",
      "gridPos": {"x": 12, "y": 10, "w": 12, "h": 6},
      "targets": [
        {"expr": "security_world_writable_files{job=\"security-81\"}", "refId": "A", "legendFormat": "World-Writable", "instant": true},
        {"expr": "security_suid_files{job=\"security-81\"}", "refId": "B", "legendFormat": "SUID Files", "instant": true}
      ],
      "options": {"orientation": "horizontal", "displayMode": "gradient", "showUnfilled": true},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 5}, {"color": "red", "value": 15}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 12, "type": "timeseries", "title": "Failed Login Attempts (Trend)",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 6},
      "targets": [{"expr": "security_failed_logins{type=\"total\",job=\"security-81\"}", "refId": "A", "legendFormat": "Total Failed Logins"}],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}},
      "fieldConfig": {"defaults": {"unit": "short", "custom": {"drawStyle": "line", "lineWidth": 2, "fillOpacity": 20}, "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 10}, {"color": "red", "value": 50}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 13, "type": "timeseries", "title": "Open Ports (Trend)",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 6},
      "targets": [{"expr": "security_open_ports{type=\"total\",job=\"security-81\"}", "refId": "A", "legendFormat": "Open Ports"}],
      "options": {"legend": {"displayMode": "list", "placement": "bottom"}},
      "fieldConfig": {"defaults": {"unit": "short", "custom": {"drawStyle": "line", "lineWidth": 2, "fillOpacity": 10}, "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 20}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 14, "type": "row", "title": "Suspicious Activity Monitoring",
      "gridPos": {"x": 0, "y": 22, "w": 24, "h": 1}, "collapsed": false
    },
    {
      "id": 15, "type": "stat", "title": "Crypto Miners",
      "gridPos": {"x": 0, "y": 23, "w": 6, "h": 4},
      "targets": [{"expr": "security_suspicious_processes{type=\"crypto_miners\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background", "textMode": "value_and_name"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"0": {"text": "NONE DETECTED", "color": "green"}}}], "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "red", "value": 1}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 16, "type": "stat", "title": "Reverse Shells",
      "gridPos": {"x": 6, "y": 23, "w": 6, "h": 4},
      "targets": [{"expr": "security_suspicious_processes{type=\"reverse_shells\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background", "textMode": "value_and_name"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"0": {"text": "NONE DETECTED", "color": "green"}}}], "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "red", "value": 1}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 17, "type": "stat", "title": "High CPU Processes",
      "gridPos": {"x": 12, "y": 23, "w": 6, "h": 4},
      "targets": [{"expr": "security_suspicious_processes{type=\"high_cpu\",job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background", "textMode": "value_and_name"},
      "fieldConfig": {"defaults": {"mappings": [{"type": "value", "options": {"0": {"text": "NORMAL", "color": "green"}}}], "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 1}, {"color": "red", "value": 3}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 18, "type": "stat", "title": "SUID Files",
      "gridPos": {"x": 18, "y": 23, "w": 6, "h": 4},
      "targets": [{"expr": "security_suid_files{job=\"security-81\"}", "refId": "A", "instant": true}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"]}, "graphMode": "none", "colorMode": "background"},
      "fieldConfig": {"defaults": {"unit": "short", "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "yellow", "value": 15}, {"color": "red", "value": 30}]}}},
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    }
  ]
}
