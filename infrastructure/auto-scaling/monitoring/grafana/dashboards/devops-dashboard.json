{
  "uid": "devops-dashboard",
  "title": "üõ†Ô∏è DevOps - Infrastructure & Operations",
  "description": "DevOps-focused metrics: uptime, deployments, resources, capacity planning",
  "tags": ["devops", "operations", "infrastructure", "sre"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "1m",
  "schemaVersion": 38,
  "version": 1,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "server",
        "type": "query",
        "query": "label_values(up{job=~\"node-exporter.*\"}, instance)",
        "datasource": {"type": "prometheus", "uid": "prometheus"},
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "üéØ SLA & Uptime Tracking",
      "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Overall Infrastructure Uptime (30d)",
      "description": "Percentage of time all monitored services were available",
      "gridPos": {"x": 0, "y": 1, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "avg_over_time(up{job=~\"node-exporter-(80|81|82|84)\"}[30d]) * 100",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["mean"]},
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 99},
              {"color": "green", "value": 99.9}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Current SLA Status",
      "description": "Target: 99.9% (43m downtime/month)",
      "gridPos": {"x": 6, "y": 1, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "(avg_over_time(up{job=~\"node-exporter-(80|81|82|84)\"}[30d]) >= 0.999) * 1",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"1": {"text": "SLA MET ‚úì", "color": "green"}}},
            {"type": "value", "options": {"0": {"text": "SLA BREACH ‚úó", "color": "red"}}}
          ]
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 4,
      "type": "bargauge",
      "title": "Uptime by Server (7d)",
      "gridPos": {"x": 12, "y": 1, "w": 12, "h": 4},
      "targets": [
        {
          "expr": "avg_over_time(up{job=~\"node-exporter-(80|81|82|84)\"}[7d]) * 100",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 99,
          "max": 100,
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 99.5},
              {"color": "green", "value": 99.9}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 5,
      "type": "row",
      "title": "üì¶ Container & Service Health",
      "gridPos": {"x": 0, "y": 5, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Total Running Containers",
      "gridPos": {"x": 0, "y": 6, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(container_last_seen{name!=\"\"})",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "green", "value": 20}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 7,
      "type": "stat",
      "title": "Container Restarts (24h)",
      "description": "Total restarts across all containers",
      "gridPos": {"x": 4, "y": 6, "w": 4, "h": 3},
      "targets": [
        {
          "expr": "sum(increase(container_restarts_total[24h]))",
          "refId": "A",
          "instant": true
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 20}
            ]
          },
          "noValue": "0"
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Container Restart Events",
      "gridPos": {"x": 8, "y": 6, "w": 16, "h": 6},
      "targets": [
        {
          "expr": "rate(container_restarts_total[5m])",
          "refId": "A",
          "legendFormat": "{{name}} ({{instance}})"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 9,
      "type": "row",
      "title": "üîã Resource Utilization & Capacity",
      "gridPos": {"x": 0, "y": 12, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "CPU Utilization Trend (All Servers)",
      "gridPos": {"x": 0, "y": 13, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "100 - (avg by(instance)(irate(node_cpu_seconds_total{mode=\"idle\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])) * 100)",
          "refId": "A",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max", "last"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Memory Utilization Trend (All Servers)",
      "gridPos": {"x": 12, "y": 13, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "100 * (1 - (node_memory_MemAvailable_bytes{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"} / node_memory_MemTotal_bytes{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}))",
          "refId": "A",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max", "last"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 80},
              {"color": "red", "value": 95}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 12,
      "type": "gauge",
      "title": "Disk Capacity Planning",
      "description": "Days until disk full (based on 7d growth rate)",
      "gridPos": {"x": 0, "y": 19, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "(node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"rootfs\",job=~\"node-exporter-(80|81|82|84)\"} / ((node_filesystem_size_bytes{mountpoint=\"/\",fstype!=\"rootfs\",job=~\"node-exporter-(80|81|82|84)\"} - node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"rootfs\",job=~\"node-exporter-(80|81|82|84)\"} - (node_filesystem_size_bytes{mountpoint=\"/\",fstype!=\"rootfs\",job=~\"node-exporter-(80|81|82|84)\"} offset 7d - node_filesystem_avail_bytes{mountpoint=\"/\",fstype!=\"rootfs\",job=~\"node-exporter-(80|81|82|84)\"} offset 7d)) / 7)) / 86400",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "d",
          "min": 0,
          "max": 90,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "orange", "value": 7},
              {"color": "yellow", "value": 30},
              {"color": "green", "value": 60}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 13,
      "type": "stat",
      "title": "Load Average (5min)",
      "gridPos": {"x": 12, "y": 19, "w": 12, "h": 3},
      "targets": [
        {
          "expr": "node_load5{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 4},
              {"color": "red", "value": 8}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 14,
      "type": "stat",
      "title": "I/O Wait Time",
      "description": "Average % CPU time waiting for I/O",
      "gridPos": {"x": 12, "y": 22, "w": 12, "h": 3},
      "targets": [
        {
          "expr": "avg by(instance)(irate(node_cpu_seconds_total{mode=\"iowait\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])) * 100",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 25}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 15,
      "type": "row",
      "title": "üåê Network Performance",
      "gridPos": {"x": 0, "y": 25, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 16,
      "type": "timeseries",
      "title": "Network Bandwidth Usage",
      "gridPos": {"x": 0, "y": 26, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(node_network_receive_bytes_total{device!~\"lo|docker.*|veth.*\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m]) * 8",
          "refId": "A",
          "legendFormat": "{{instance}} RX"
        },
        {
          "expr": "rate(node_network_transmit_bytes_total{device!~\"lo|docker.*|veth.*\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m]) * 8",
          "refId": "B",
          "legendFormat": "{{instance}} TX"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "max"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 15
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 17,
      "type": "timeseries",
      "title": "Network Errors & Drops",
      "gridPos": {"x": 12, "y": 26, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(node_network_receive_errs_total{device!~\"lo|docker.*|veth.*\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])",
          "refId": "A",
          "legendFormat": "{{instance}} RX Errors"
        },
        {
          "expr": "rate(node_network_transmit_errs_total{device!~\"lo|docker.*|veth.*\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])",
          "refId": "B",
          "legendFormat": "{{instance}} TX Errors"
        },
        {
          "expr": "rate(node_network_receive_drop_total{device!~\"lo|docker.*|veth.*\",job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])",
          "refId": "C",
          "legendFormat": "{{instance}} RX Drops"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "pps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 18,
      "type": "row",
      "title": "‚öôÔ∏è System Operations",
      "gridPos": {"x": 0, "y": 32, "w": 24, "h": 1},
      "collapsed": false
    },
    {
      "id": 19,
      "type": "stat",
      "title": "System Context Switches",
      "description": "Context switches per second (high values may indicate CPU contention)",
      "gridPos": {"x": 0, "y": 33, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rate(node_context_switches_total{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10000},
              {"color": "red", "value": 50000}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 20,
      "type": "stat",
      "title": "Open File Descriptors",
      "description": "Current open FDs vs max allowed",
      "gridPos": {"x": 6, "y": 33, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "node_filefd_allocated{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"} / node_filefd_maximum{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"} * 100",
          "refId": "A",
          "instant": true,
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "red", "value": 80}
            ]
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    },
    {
      "id": 21,
      "type": "timeseries",
      "title": "System Interrupts",
      "gridPos": {"x": 12, "y": 33, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(node_intr_total{job=~\"node-exporter-(80|81|82|84)\",instance=~\"$server\"}[5m])",
          "refId": "A",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "tooltip": {"mode": "multi"},
        "legend": {"displayMode": "table", "placement": "right"}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          }
        }
      },
      "datasource": {"type": "prometheus", "uid": "prometheus"}
    }
  ]
}
